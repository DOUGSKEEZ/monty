{
  "rewrites": [
    { "source": "/guest-qr-codes", "destination": "/guest-qr-codes.html" },
    { "source": "/guest-qr-codes.html", "destination": "/guest-qr-codes.html" },
    { "source": "**", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "**/*.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ]
}
